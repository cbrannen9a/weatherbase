{
	"name": "weatherbase",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"isomorphic-fetch": "^2.2.1",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-router-dom": "^4.3.1"
	},
	"devDependencies": {
		"react-scripts": "2.1.3"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"deploy": "yarn build && firebase deploy --only hosting"
	},
	"eslintConfig": {
		"extends": "react-app",
		"rules": {
			"semi": [
				2,
				"always"
			],
			"quotes": [
				2,
				"single",
				{
					"avoidEscape": true
				}
			]
		}
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
